<form method="post" action="{{ path('app_city_delete', {'id': city.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer la ville de {{ city.name }} ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ city.id) }}">
    <button class="btn">Supprimer</button>
</form>
